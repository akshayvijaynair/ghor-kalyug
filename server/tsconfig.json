{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "target": "ES6",
    "module": "commonjs",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "paths": {}
  },
  "include": ["src", "src/**/*.ts","src/*.d.ts"],
  "exclude": ["dist"]
}
